<div x-init="title = 'Update Order'">
  <.form
    let={f}
    for={@changeset}
    id="edit-order-form"
    phx_target={@myself}
    phx_change="validate"
    phx_submit="submit"
  >
    <KamanskyWeb.CustomerLive.RawFormComponent.customer_info customer={@customer} />
    <KamanskyWeb.OrderLive.Components.order_form 
      f={f}
      changeset={@changeset}
    />
  </.form>
</div>